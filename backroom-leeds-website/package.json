{
  "name": "backroom-leeds-website",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:report": "npx playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:restart": "supabase stop && supabase start",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:seed": "supabase db reset --exclude-schema=extensions --exclude-schema=auth --exclude-schema=storage",
    "supabase:generate-types": "supabase gen types typescript --local --schema public > src/types/database.types.ts",
    "supabase:studio": "open http://127.0.0.1:54323",
    "supabase:test": "node scripts/test-supabase.js",
    "test:integration": "node scripts/test-nextjs-supabase.js",
    "test:full-stack": "npm run supabase:test && npm run test:integration",
    "test:database": "NODE_OPTIONS='--experimental-vm-modules' jest tests/database/",
    "validate-setup": "node scripts/validate-setup.js",
    "db:migrate": "supabase db diff --use-migra && supabase db push",
    "db:push": "supabase db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "clsx": "^2.1.1",
    "next": "^15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-specific-snapshot": "^8.0.0",
    "msw": "^2.10.5",
    "postcss": "^8.5.6",
    "supabase": "^2.34.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
