  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test:accessibility": "jest src/tests/accessibility.test.tsx",
    "audit:performance": "node performance-audit.js",
    "audit:full": "npm run test:accessibility && npm run audit:performance && npm run test:ci",
    "test:e2e": "npx playwright test",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:integration": "jest -c jest.config.integration.ts",
    "test:performance": "vitest bench run",
    "test:e2e:ci": "playwright test",
    "test:ci": "jest && vitest run && playwright test",
    "test:full-stack": "npm run supabase:test && npm run test:integration && npm run test:e2e",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:restart": "supabase stop && supabase start",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:seed": "supabase db reset --exclude-schema=extensions --exclude-schema=auth --exclude-schema=storage",
    "supabase:generate-types": "supabase gen types typescript --local --schema public > src/types/database.types.ts",
    "supabase:studio": "open http://127.0.0.1:54323",
    "supabase:test": "node scripts/test-supabase.js",
    "test:database": "NODE_OPTIONS='--experimental-vm-modules' jest tests/database/",
    "validate-setup": "node scripts/validate-setup.js",
    "db:migrate": "supabase db diff --use-migra && supabase db push",
    "db:push": "supabase db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
